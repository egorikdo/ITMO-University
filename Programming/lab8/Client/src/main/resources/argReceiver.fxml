<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox prefHeight="40.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.14-internal"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.ArgumentReceiverController" spacing="20">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10"/>
    </padding>
    <TextField fx:id="field" promptText="enter argument"/>
    <HBox spacing="70">
        <padding>
            <Insets bottom="10" left="10" right="10" top="10"/>
        </padding>
        <Button text="Cancel" onAction="#cancelButtonOnClick"/>
        <Button text="OK" onAction="#readArgs"/>
    </HBox>
</VBox>
